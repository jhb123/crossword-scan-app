<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/gridScanFragment">


    <fragment
        android:id="@+id/clueScanFragment"
        android:name="com.example.learn_opencv.ClueScanFragment"
        android:label="fragment_clue_scan"
        tools:layout="@layout/fragment_clue_scan" >
        <action
            android:id="@+id/action_clueScanFragment_to_gridScanFragment3"
            app:destination="@id/gridScanFragment" />
        <action
            android:id="@+id/action_clueScanFragment_to_puzzleSelectFragment2"
            app:destination="@id/puzzleSelectFragment" />
    </fragment>
    <fragment
        android:id="@+id/gridScanPreviewFragment"
        android:name="com.example.learn_opencv.GridScanPreviewFragment"
        android:label="fragment_grid_scan_preview"
        tools:layout="@layout/fragment_grid_scan_preview" >
        <action
            android:id="@+id/action_gridScanPreviewFragment_to_gridScanFragment3"
            app:destination="@id/gridScanFragment" />
        <action
            android:id="@+id/action_gridScanPreviewFragment_to_clueScanFragment3"
            app:destination="@id/clueScanFragment" />
        <action
            android:id="@+id/action_gridScanPreviewFragment_to_puzzleSelectFragment2"
            app:destination="@id/puzzleSelectFragment" />
    </fragment>
    <fragment
        android:id="@+id/gridScanFragment"
        android:name="com.example.learn_opencv.GridScanFragment"
        android:label="fragment_grid_scan"
        tools:layout="@layout/fragment_grid_scan" >
        <action
            android:id="@+id/action_gridScanFragment_to_clueScanFragment3"
            app:destination="@id/clueScanFragment" />
        <action
            android:id="@+id/action_gridScanFragment_to_gridScanPreviewFragment3"
            app:destination="@id/gridScanPreviewFragment" />
        <action
            android:id="@+id/action_gridScanFragment_to_puzzleSelectFragment2"
            app:destination="@id/puzzleSelectFragment" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.learn_opencv.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/puzzleSelectFragment"
        android:name="com.example.learn_opencv.puzzleSelectFragment"
        android:label="fragment_puzzle_select"
        tools:layout="@layout/fragment_puzzle_select" >
        <action
            android:id="@+id/action_puzzleSelectFragment2_to_solveFragment2"
            app:destination="@id/solveFragment" />
        <action
            android:id="@+id/action_puzzleSelectFragment2_to_gridScanPreviewFragment"
            app:destination="@id/gridScanPreviewFragment" />
        <action
            android:id="@+id/action_puzzleSelectFragment2_to_gridScanFragment"
            app:destination="@id/gridScanFragment" />
        <action
            android:id="@+id/action_puzzleSelectFragment2_to_clueScanFragment"
            app:destination="@id/clueScanFragment" />
    </fragment>
    <fragment
        android:id="@+id/solveFragment"
        android:name="com.example.learn_opencv.SolveFragment"
        android:label="fragment_solve"
        tools:layout="@layout/fragment_solve" >
        <action
            android:id="@+id/action_solveFragment2_to_puzzleSelectFragment2"
            app:destination="@id/puzzleSelectFragment" />
    </fragment>
</navigation>